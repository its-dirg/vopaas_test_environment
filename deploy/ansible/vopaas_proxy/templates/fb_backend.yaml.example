google_backend.yaml.examplemodule: satosa.backends.oauth.FacebookBackend
plugin: BackendModulePlugin
name: facebook
config:
  authz_page: <name>
  base_url: <base_url>
  client_config: {client_id: ''}
  client_secret:
  fields: [id, name, first_name, last_name, middle_name, picture, email, verified,
    gender, timezone, locale, updated_time]
  response_type: code
  server_info: {authorization_endpoint: 'https://www.facebook.com/dialog/oauth', token_endpoint: 'https://graph.facebook.com/v2.5/oauth/access_token'}
  state_id: <name>
  verify_accesstoken_state: false
  op_info:
    organization:
      display_name:
      - ["OP Facebook Identities", "en"]
      name:
      - ["En facebook test-OP", "en"]
      url:
      - ["https://www.facebook.com/", "en"]
    ui_info:
      description:
      - ["This is a facebook test OP", "en"]
      display_name:
      - ["OP - FACEBOOK - TEST", "en"]
      logo:
      - image: "data:image/png;base64,"
        width: "120"
        height: "60"
        lang: "en"

    contact_person:
      - contact_type: "technical"
        given_name: "Test"
        sur_name: "FACEBOOK"
        email_address: ["technical_test@example.com", "support_test@example.com"]
      - contact_type: "support"
        given_name: "Support"
        email_address: ["support@example.com"]

